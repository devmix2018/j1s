<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

   
    <route id="Import" pattern="/">
        <default key="_controller">TuniAnnonceBundle:Configuration:index</default>
        <requirement key="_method">GET</requirement>
    </route>
    <route id="importRegionVille" pattern="/importRegionVille">
        <default key="_controller">TuniAnnonceBundle:Configuration:importRegionVille</default>
        <requirement key="_method">POST</requirement>
    </route>

    <route id="importCategories" pattern="/importCategories">
        <default key="_controller">TuniAnnonceBundle:Configuration:importCategories</default>
        <requirement key="_method">POST</requirement>
    </route>



</routes>
