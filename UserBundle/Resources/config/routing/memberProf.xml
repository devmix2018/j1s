<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="tuni_membreProf" pattern="/">
        <default key="_controller">TuniAnnonceBundle:MembreProf:index</default>
        <requirement key="_method">GET</requirement>
    </route>
    <route id="tuni_membreProf_create" pattern="/create">
        <default key="_controller">TuniAnnonceBundle:MembreProf:create</default>
        <requirement key="_method">POST</requirement>
    </route>

    <route id="tuni_membreProf_new" pattern="/new">
        <default key="_controller">TuniAnnonceBundle:MembreProf:new</default>
        <requirement key="_method">GET</requirement>
    </route>

    <route id="tuni_membreProf_show" pattern="/{id}/show">
        <default key="_controller">TuniAnnonceBundle:MembreProf:show</default>
        <requirement key="_method">GET</requirement>
    </route>
    
    <route id="tuni_membreProf_edit" pattern="/{id}/edit">
        <default key="_controller">TuniAnnonceBundle:MembreProf:edit</default>
        <requirement key="_method">GET</requirement>
    </route>
    
    <route id="tuni_membreProf_update" pattern="/{id}/update">
        <default key="_controller">TuniAnnonceBundle:MembreProf:update</default>
        <requirement key="_method">POST</requirement>
    </route>
    
    <route id="tuni_membreProf_delete" pattern="/{id}/delete">
        <default key="_controller">TuniAnnonceBundle:MembreProf:delete</default>
         <requirement key="_method">GET</requirement>
    </route>


</routes>
