{% extends "TuniAnnonceBundle:Default:index.html.twig" %}
{% block maincontent %}
  <section class="main-content grid_24 alpha  ">

   
<form class="form-horizontal" action="{{ path('membreProf_create') }}" {{ form_enctype(form) }}{{ form_enctype(formMembre) }} method="POST" >
    
        
				<div class="grid_22 widget-head">
					<h4 class="heading glyphicons edit"><i></i>Ajout nouveau Employeur</h4>
				</div>
				    {% for type, messages in app.session.flashBag.all %}
                                        {% for message in messages %}
                                            <div class="{{ type }} alert-success">
                                                {{ message|trans({}, 'FOSUserBundle') }}
                                            </div>
                                        {% endfor %}
                                    {% endfor %}
					   {% include "TuniAnnonceBundle:MembreProf:new_content.html.twig" %}
					
					
				
		
			
					<div class="grid_22 form-actions" style="margin: 0;">
						<button type="submit" class="btn btn-icon btn-primary glyphicons circle_ok"><i></i>{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}</button>
                                                <button type="reset" style="float:right;" class="btn btn-icon btn-default glyphicons circle_remove"><i></i>Annuler</button>
                                         </div>
					
				
			
		
	 
{{ form_rest(form) }}	    
</form>


  </section>
                
{% endblock %}
