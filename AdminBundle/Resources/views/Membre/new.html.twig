{% extends "TuniAdminBundle::layout.html.twig" %}

{% block content_middle_b %}
<ul class="breadcrumb">
	<li><a  class="glyphicons home"><i></i> juste1service</a></li>
	<li class="divider"></li>
	<li>{{'Administrateur'|trans({}, 'TuniAdminBundle') }}</li>
</ul>
<div class="separator"></div>

<div class="heading-buttons">
    <h3 class="glyphicons group"><i></i> Jobeurs</h3>
    
    <div class="clearfix"></div>
</div>
<div class="separator"></div>

<div class="innerLR">
   
<form class="form-horizontal" action="{{ path('membre_create') }}" {{ form_enctype(form) }}{{ form_enctype(formMembre) }} method="POST" >
    
        
    
    <div class="" style="padding: 0;" >
		<div class="" id="account-details">
		
			<div class="widget widget-2">
				<div class="widget-head">
					<h4 class="heading glyphicons edit"><i></i>Ajout nouveau Jobeur</h4>
				</div>
				<div class="widget-body" style="padding-bottom: 0;">
                                    {% for type, messages in app.session.flashBag.all %}
                                        {% for message in messages %}
                                            <div class="{{ type }} alert-success">
                                                {{ message|trans({}, 'FOSUserBundle') }}
                                            </div>
                                        {% endfor %}
                                    {% endfor %}
					<div class="row-fluid">
                                           {% include "TuniAdminBundle:Membre:new_content.html.twig" %}
					</div>
					
					
				
		
			
					<div class="form-actions" style="margin: 0;">
						<button type="submit" class="btn btn-icon btn-primary glyphicons circle_ok"><i></i>{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}</button>
                                                <button type="reset" style="float:right;" class="btn btn-icon btn-default glyphicons circle_remove" ><i></i>Annuler</button>
                                         </div>
                                                
					
				</div>
			</div>
		</div>
			
		
	</div>
{{ form_rest(form) }}	    
</form>
</div>
{% endblock %}
